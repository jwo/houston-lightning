LightningTalks.config ($routeProvider, $locationProvider) ->
  $routeProvider
    .when "/",
      template: JST['angular/templates/dashboard/dashboard']
      controller: "DashboardController"
    .when "/register",
      template: JST['angular/templates/dashboard/new']
      controller: "TalkController"
    .when '/admin',
      template: JST['angular/templates/admin/admin-dashboard']
      controller: 'AdminController'
    .otherwise redirectTo: "/"
  $locationProvider.html5Mode(true)
